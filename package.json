    ],
    "verbose": true
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "proseWrap": "always",
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false
