    "prettier": "prettier --write \"{src,test}/**/*.{js,jsx,css}\""
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./test/setup.js",
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
